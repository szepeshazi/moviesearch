{
  "name": "moviesearch",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod",
    "clean:target": "sudo rm -R $npm_package_config_target_dir/*",
    "clean": "npm-run-all clean:*",
    "deploy:html": "sudo cp $npm_package_config_out_dir/*.html $npm_package_config_target_dir/",
    "deploy:css": "sudo cp $npm_package_config_out_dir/*.css $npm_package_config_target_dir/ && sudo cp $npm_package_config_out_dir/*.css.gz $npm_package_config_target_dir/",
    "deploy:js": "sudo cp $npm_package_config_out_dir/*.js $npm_package_config_target_dir/ && sudo cp $npm_package_config_out_dir/*.js.gz $npm_package_config_target_dir/",
    "deploy:icon": "sudo cp $npm_package_config_out_dir/favicon.ico $npm_package_config_target_dir/",
    "deploy:images": "sudo cp -R public/images $npm_package_config_target_dir/",
    "deploy:htaccess": "sudo cp _htaccess.dist $npm_package_config_target_dir/.htaccess",
    "deploy": "npm-run-all deploy:*"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "git+https://github.com/angular/material2-builds.git",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular2-infinite-scroll": "^0.3.4",
    "core-js": "^2.4.1",
    "npm-run-all": "^4.0.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
